{
  "name": "react-antd-template",
  "version": "0.1.2",
  "description": "React Ant Design Template",
  "entry": {
    "index": "./src/entry/index.jsx"
  },
  "externals": {
    "antd": "antd",
    "react": "React",
    "react-dom": "ReactDOM"
  },
  "dependencies": {
    "antd": "^1.1.0",
    "jquery-ajax": "^2.1.4",
    "koa-static": "^2.0.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-router": "^2.4.1"
  },
  "devDependencies": {
    "apack-server": "^1.0.0",
    "apack-build": "^1.0.0"
  },
  "scripts": {
    "dev": "apack-server --config ./dev.js --browser-history --live-reload --mock ./mock",
    "dev:remote": "apack-server --remote https://myserver.com",
    "build": "apack-build -o ./dist"
  }
